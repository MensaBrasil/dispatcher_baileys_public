# Environment settings for Dispatcher (Baileys WhatsApp)
# Copy this file to `.env` and adjust values as needed.

# General
NODE_ENV=development

# App logging (pino)
# One of: fatal, error, warn, info, debug, trace, silent
LOG_LEVEL=info
# Pretty logs in development. Set to "false" in production.
PRETTY_LOGS=true

# Baileys internal logger level
# One of: fatal, error, warn, info, debug, trace, silent
BAILEYS_LOG_LEVEL=fatal
